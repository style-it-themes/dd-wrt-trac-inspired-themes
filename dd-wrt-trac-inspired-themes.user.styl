  // Better checkbox/radio inputs.
  input {
    r       = unquote
    prefix  = 'data:image/svg+xml;utf8,'
    prefix += '<svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 512 512">'
    &[type='radio'],
    &[type='checkbox'] {
      &, &:focus, &:hover:active {
        -webkit-appearance: none
        -moz-appearance: none
        appearance: none
        cursor: pointer
        height: b_hgwd
        width: b_hgwd
        border: 1px solid bg5
        background-size: cover
        transition: transition
        c: v08 0 bg1
      }
      &:disabled, &:hover:active:disabled, &:disabled:hover {
        c: v94 bg5 bg4
        cursor: default
      }
    }

    &[type='radio'] {
      border-radius: 50%
      padding: 7px
      &:checked {
        path = '<circle cx="256" cy="256" r="128" fill="' + to_svg($v99) + '"/>'
        background-image: s("url('%s%s</svg>') %s", r(prefix), r(path), i)
        c: 0 v08 v09
      }
      &:focus, &:hover { c: 0 v09 bg2 }
    }

    &[type='checkbox'] {
      border-radius: 2px
      &:checked {
        path ='<path fill="' + to_svg($v99) + '" d="M99 213l-1 89 104 102 205-201v-91L203 315 99 213z"/>'
        background-image: s("url('%s%s</svg>') %s", r(prefix), r(path), i)
        c: 0 v08 v09
        rad: 2px
      }
      &:focus, &:hover { c: 0 v09 bg2 }
      &:indeterminate {
        path = '<rect x="128" y="128" width="256" height="256" fill="' + to_svg($v99) + '"/>'
        background-image: s("url('%s%s</svg>') %s", r(prefix), r(path), i)
        c: 0 v08 v09
        &:hover { c: 0 v09 bg2 }
      }
    }

    &[type='file'] {
      &::file-selector-button {
        background-color: bg5
        border: 2px solid bg5
        color: v97

        border-radius: .2em
        padding: .2rem .4rem
        cursor: pointer
        &:hover {
          background-color: v07
          border: 2px solid v08
          color: v99
        }
      }
      &::-webkit-file-upload-button {
        background-color: bg5
        border: 2px solid bg5
        color: v98

        border-radius: .2em
        padding: .2rem .4rem
        cursor: pointer
        &:hover {
          background-color: v07
          border: 2px solid v08
          color: v99
        }
      }
    }
    &[type='submit'] {
      background-color: bg5
      border: 1px solid bg5
      color: v98
      box-shadow: none
      padding: .25rem .8rem .35rem .8rem
      border-radius: .2em
      &[value="Login"] {
        margin-top: 1rem
      }
      &[disabled], &[disabled]:hover {
        background: bg2
        border-color: bg3
        color: v94
        cursor: unset
      }
      &:hover {
        background-color: v08
        border: 1px solid v09
        color: v99
        box-shadow: none
        text-shadow: none
      }
      &#attachfilebutton {
        margin-left: -.8rem
      }
    }
    &[type='text'], &[type="password"] {
      background-color: bg0
      border: 1px solid bg4
      color: v98
      box-shadow: none
      padding: .25rem .8rem .25rem .2rem
      border-radius: .2em
      transition: transition
      &:focus, &:focus-within, &:focus-visible {
        c: 0 v09 bg1
        outline: 0
      }
    }
    // audit log export inut fields
    &[id="start-time"], &[id="end-time" i] {
      -webkit-appearance: none
      -moz-appearance: none
      appearance: none
      background: bg0
      border: 1px solid bg5
      color: v99
      &:active, &:focus-visible {
        border-color: v08
        outline: none
      }
    }
  }

    // Better select element styles.
  /select {
    & {
      background: bg0
      // Override default colors.
      border: 1px solid bg4
      color: v98
      transition: transition
      &:not([disabled]) {
        c: 0 0 bg0
        &:hover { c: 0 0 bg2 }
        &:active { c: 0 0 bg0 }
      }
      option {
        &[selected] {
          background: v05
        }
        &:hover { background-color: v02 }
        &:checked {
          background-color: v05 i
          box-shadow: 0 0 10px 100px v06 inset
        }
      }
    }
    &[disabled] {
      background: bg2
    }
  }
}
